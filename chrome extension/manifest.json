{
    "manifest_version": 2,
    "name": "CHANGE THIS",
    "description":"CHANGE THIS.",
    "version": "0.1",
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_title": "CHANGE THIS"
    },
      "commands": {
        "_execute_browser_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+C",
            "mac": "MacCtrl+Shift+C"
          }
        }
      },
      "permissions": [
        "tabs"
      ]
  }